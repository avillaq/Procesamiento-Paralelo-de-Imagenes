{
  "id": null,
  "title": "Sistema Distribuido - Procesamiento de Imágenes",
  "description": "Monitoreo completo del sistema distribuido",
  "tags": ["sistema-distribuido", "procesamiento-imagenes"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "title": "Nodos Activos",
      "type": "stat",
      "targets": [
        {
          "expr": "nodos_activos",
          "legendFormat": "Nodos Activos",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 3},
              {"color": "green", "value": 4}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Coordinador Actual",
      "type": "stat",
      "targets": [
        {
          "expr": "coordinador_actual_nodo_id",
          "legendFormat": "Coordinador",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "fixed", "fixedColor": "blue"}
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "Peticiones HTTP del Cliente",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(total_peticiones_http_total{job=\"cliente-flask\"}[5m])",
          "legendFormat": "{{metodo}} {{endpoint}} ({{codigo_estado}})",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "reqps",
          "label": "Peticiones/segundo"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "Imágenes Procesadas - Nodos",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(total_imagenes_procesadas_total{job=\"nodos-procesamiento\"}[5m])",
          "legendFormat": "Nodo {{nodo_id}} - {{estado}}",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "ops",
          "label": "Imágenes/segundo"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 5,
      "title": "Imágenes Subidas - Cliente",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(total_imagenes_subidas_total{job=\"cliente-flask\"}[5m])",
          "legendFormat": "{{estado}}",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "ops",
          "label": "Imágenes/segundo"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "id": 6,
      "title": "Tiempo de Procesamiento - Cliente",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(duracion_procesamiento_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p50 - Cliente",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(duracion_procesamiento_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p95 - Cliente",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(duracion_procesamiento_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p99 - Cliente",
          "refId": "C"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Tiempo (segundos)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 7,
      "title": "Tiempo de Procesamiento - Nodos",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(duracion_procesamiento_bucket{job=\"nodos-procesamiento\"}[5m]))",
          "legendFormat": "p50 - Nodo {{nodo_id}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(duracion_procesamiento_bucket{job=\"nodos-procesamiento\"}[5m]))",
          "legendFormat": "p95 - Nodo {{nodo_id}}",
          "refId": "B"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Tiempo (segundos)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    },
    {
      "id": 8,
      "title": "Peticiones gRPC por Nodo",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(total_peticiones_grpc_total{job=\"nodos-procesamiento\"}[5m])",
          "legendFormat": "Nodo {{nodo_id}} - {{metodo}} ({{estado}})",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "reqps",
          "label": "Peticiones/segundo"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
    },
    {
      "id": 9,
      "title": "Latencia HTTP - Cliente",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(duracion_peticiones_http_segundos_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p50 HTTP",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(duracion_peticiones_http_segundos_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p95 HTTP",
          "refId": "B"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Tiempo (segundos)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
    },
    {
      "id": 10,
      "title": "Uso de CPU por Nodo",
      "type": "graph",
      "targets": [
        {
          "expr": "porcentaje_uso_cpu{job=\"nodos-procesamiento\"}",
          "legendFormat": "Nodo {{nodo_id}}",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "percent",
          "max": 100,
          "min": 0
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
    },
    {
      "id": 11,
      "title": "Uso de Memoria por Nodo",
      "type": "graph",
      "targets": [
        {
          "expr": "porcentaje_uso_memoria{job=\"nodos-procesamiento\"}",
          "legendFormat": "Nodo {{nodo_id}}",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "percent",
          "max": 100,
          "min": 0
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
    },
    {
      "id": 12,
      "title": "Estado de GlusterFS",
      "type": "stat",
      "targets": [
        {
          "expr": "estado_glusterfs{job=\"cliente-flask\"}",
          "legendFormat": "GlusterFS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"options": {"0": {"text": "Offline"}}, "type": "value"},
            {"options": {"1": {"text": "Online"}}, "type": "value"}
          ],
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 40}
    },
    {
      "id": 13,
      "title": "Elecciones Bully",
      "type": "graph",
      "targets": [
        {
          "expr": "increase(total_elecciones_bully_total{job=\"nodos-procesamiento\"}[5m])",
          "legendFormat": "Nodo {{nodo_id}} - {{resultado}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 18, "x": 6, "y": 40}
    },
    {
      "id": 14,
      "title": "Operaciones GlusterFS",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(total_operaciones_glusterfs_total{job=\"cliente-flask\"}[5m])",
          "legendFormat": "{{operacion}} ({{estado}})",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
    },
    {
      "id": 15,
      "title": "Tiempo de Búsqueda de Coordinador",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(tiempo_busqueda_coordinador_segundos_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(tiempo_busqueda_coordinador_segundos_bucket{job=\"cliente-flask\"}[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Tiempo (segundos)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "editable": true
}