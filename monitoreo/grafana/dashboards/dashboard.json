{
  "id": null,
  "title": "Sistema Distribuido - Procesamiento Paralelo de Imágenes",
  "description": "Métricas críticas del sistema distribuido",
  "tags": ["sistema-distribuido", "procesamiento-imagenes", "rendimiento"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 2,
  "panels": [
    {
      "id": 1,
      "title": "Nodos Activos",
      "type": "stat",
      "targets": [
        {
          "expr": "nodos_activos",
          "legendFormat": "Nodos Activos",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 2},
              {"color": "green", "value": 3}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Coordinador Actual",
      "type": "stat",
      "targets": [
        {
          "expr": "coordinador_actual_nodo_id_info",
          "legendFormat": "Coordinador",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "fixed", "fixedColor": "blue"}
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 4, "y": 0}
    },
    {
      "id": 3,
      "title": "Estado GlusterFS",
      "type": "stat",
      "targets": [
        {
          "expr": "estado_glusterfs{job=\"cliente-flask\"}",
          "legendFormat": "GlusterFS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"options": {"0": {"text": "Offline"}}, "type": "value"},
            {"options": {"1": {"text": "Online"}}, "type": "value"}
          ],
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 8, "y": 0}
    },
    {
      "id": 4,
      "title": "Speedup Paralelo",
      "type": "graph",
      "targets": [
        {
          "expr": "speedup_paralelo",
          "legendFormat": "{{num_nodos}} nodos",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "short",
          "label": "Factor de mejora"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0}
    },
    {
      "id": 5,
      "title": "Throughput - Imágenes Subidas vs Procesadas",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(total_imagenes_subidas_total{job=\"cliente-flask\"}[5m])",
          "legendFormat": "Subidas",
          "refId": "A"
        },
        {
          "expr": "sum(rate(total_imagenes_procesadas_total{job=\"nodos-procesamiento\"}[5m]))",
          "legendFormat": "Procesadas (Total)",
          "refId": "B"
        }
      ],
      "yAxes": [
        {
          "unit": "ops",
          "label": "Imágenes/segundo"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
    },
    {
      "id": 6,
      "title": "Latencia de Procesamiento - Comparativa",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(duracion_procesamiento_cliente_bucket[5m]))",
          "legendFormat": "p50 - Cliente (Total)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(duracion_procesamiento_cliente_bucket[5m]))",
          "legendFormat": "p95 - Cliente (Total)",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.50, rate(duracion_procesamiento_nodo_bucket[5m]))",
          "legendFormat": "p50 - Nodos (Individual)",
          "refId": "C"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Tiempo (segundos)"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 14}
    },
    {
      "id": 7,
      "title": "Estado de Coordinadores",
      "type": "graph",
      "targets": [
        {
          "expr": "es_coordinador",
          "legendFormat": "Nodo {{nodo_id}}",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "unit": "short",
          "max": 1.2,
          "min": -0.2
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22}
    },
    {
      "id": 8,
      "title": "Algoritmo Bully - Actividad",
      "type": "graph",
      "targets": [
        {
          "expr": "increase(total_elecciones_bully_total[5m])",
          "legendFormat": "Elecciones - {{resultado}}",
          "refId": "A"
        },
        {
          "expr": "increase(total_cambios_coordinador_total[5m])",
          "legendFormat": "Cambios {{antiguo_c}}→{{nuevo_c}}",
          "refId": "B"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30}
    },
    {
      "id": 9,
      "title": "Tiempo de Búsqueda de Coordinador",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(tiempo_busqueda_coordinador_segundos_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(tiempo_busqueda_coordinador_segundos_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Tiempo (segundos)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "editable": true
}